{
 "name": "n8n-nodes-leadtable",
 "version": "1.0.0",
 "description": "Seamless integration with LeadTable API â€“ powered by agentur-systeme.de",
 "license": "MIT",
 "homepage": "https://github.com/redhat030/n8n-nodes-leadtable#readme",
 "author": "agentur-systeme.de",
 "repository": {
   "type": "git",
   "url": "https://github.com/redhat030/n8n-nodes-leadtable.git"
 },
 "bugs": {
   "url": "https://github.com/redhat030/n8n-nodes-leadtable/issues"
 },
 "engines": {
   "node": ">=18.10",
   "npm": ">=9.0.0"
 },
 "main": "index.js",
 "scripts": {
   "build": "tsc && npm run copy:assets",
   "copy:assets": "find nodes -name '*.svg' -exec cp {} dist/nodes/LeadTable/ \\;",
   "dev": "tsc --watch",
   "clean": "rm -rf dist",
   "prebuild": "npm run clean",
   "prepublishOnly": "npm run build"
 },
 "files": [
   "dist/**/*",
   "README.md",
   "LICENSE"
 ],
 "n8n": {
   "n8nNodesApiVersion": 1,
   "credentials": [
     "dist/credentials/LeadTableApi.credentials.js"
   ],
   "nodes": [
     "dist/nodes/LeadTable/LeadTable.node.js",
     "dist/nodes/LeadTable/LeadTableTrigger.node.js"
   ]
 },
 "keywords": [
   "n8n-community-node-package",
   "n8n",
   "leadtable",
   "lead-management", 
   "webhook",
   "automation",
   "crm",
   "workflow",
   "agentur-systeme.de"
 ],
 "dependencies": {
   "n8n-workflow": "^1.0.0"
 },
 "devDependencies": {
   "@types/node": "^18.16.16",
   "typescript": "^5.1.3"
 },
 "peerDependencies": {
   "n8n-workflow": "*"
 }
}