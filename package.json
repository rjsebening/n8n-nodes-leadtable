{
  "name": "n8n-nodes-leadtable",
  "version": "1.0.4",
  "description": "n8n custom node for LeadTable API - Lead management and webhook automation",
  "license": "MIT",
  "homepage": "https://github.com/rjsebening/n8n-nodes-leadtable#readme",
  "author": {
    "name": "JÃ¶rg Sebening",
    "email": "dev@joerg-sebening.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjsebening/n8n-nodes-leadtable.git"
  },
  "bugs": {
    "url": "https://github.com/rjsebening/n8n-nodes-leadtable/issues"
  },
  "engines": {
    "node": ">=20.15"
  },
  "scripts": {
    "build": "npx rimraf dist && tsc",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/LeadTableApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LeadTable/LeadTable.node.js",
      "dist/nodes/LeadTable/LeadTableTrigger.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "leadtable",
    "lead-management",
    "webhook",
    "automation",
    "crm",
    "workflow"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~8.32.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "exit": "^0.1.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
